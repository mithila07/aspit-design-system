{
  "name": "design-system-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pack_lib": "npm run build_lib && npm run css_minify && npm run copy_css && npm run copy_fontello_fonts && cd dist/design-system && npm pack",
    "copy_styles": "cross-env-shell \"copy \"projects/design-system/src/lib/*\" \"dist/design-system/bundles\"",
    "copy_abstract": "cross-env-shell \"copy \"projects/design-system/src/lib/scss/abstract/*\" \"dist/design-system/bundles/scss/abstract\"",
    "copy_base": "cross-env-shell \"copy \"projects/design-system/src/lib/scss/base/\" \"dist/design-system/bundles/scss/base\"",
    "copy_components": "cross-env-shell \"copy \"projects/design-system/src/lib/scss/components/*\" \"dist/design-system/bundles/scss/components\"",
    "copy_modules": "cross-env-shell \"copy \"projects/design-system/src/lib/scss/modules/*\" \"dist/design-system/bundles/scss/modules\"",
    "copy_vendors": "cross-env-shell \"copy \"projects/design-system/src/lib/scss/vendors/*\" \"dist/design-system/bundles/scss/vendors\"",
    "build_lib": "ng build design-system --prod",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pack_all": "cd dist/design-system && npm pack",
    "css_minify": "npm run css_minify_aspitnew",
    "css_minify_aspitnew": "node-sass projects/design-system/src/lib/aspit-new.scss projects/design-system/src/lib/aspit-design-system.min.css --output-style compressed",
    "css_watch_aspitnew": "node-sass -w projects/design-system/src/lib/aspit-new.scss projects/design-system/src/lib/aspit-design-system.min.css --output-style compressed",
    "copy_fontello": "cross-env-shell rsync -av projects/design-system/src/lib/fonts dist/design-system/bundles",
    "copy_fontello_fonts": "cross-env-shell rsync -av projects/design-system/src/lib/fonts dist/design-system/bundles",
    "copy_css": "cross-env-shell rsync -av projects/design-system/src/lib/*.min.css dist/design-system/bundles",
    "css_minify_wrapper": "node-sass  projects/design-system/src/lib/aspit_wrapper.scss projects/design-system/src/lib/asw.min.css --output-style compressed"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ng-select/ng-select": "^3.0.7",
    "@storybook/addon-storysource": "^5.2.5",
    "angular-gridster2": "^9.1.0",
    "bootstrap": "^4.5.2",
    "core-js": "^2.5.4",
    "highlight.js": "9.16.2",
    "highlightjs-line-numbers.js": "2.7.0",
    "material-icons": "^0.3.1",
    "ng5-slider": "^1.2.4",
    "ngx-bootstrap": "^5.6.1",
    "ngx-highlightjs": "4.0.0",
    "ngx-perfect-scrollbar": "^10.0.1",
    "ngx-toastr": "11.3.3",
    "ngx-virtual-scroller": "^4.0.3",
    "node-sass": "^4.14.1",
    "rxjs": "^6.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.19",
    "@angular-devkit/build-ng-packagr": "~0.803.19",
    "@angular/cli": "^8.3.19",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@babel/core": "^7.6.4",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addon-notes": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/angular": "^5.2.5",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.0.1",
    "cross-env": "^5.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": "^0.37.0",
    "tslib": "^1.10.0",
    "tslint": "~5.9.1",
    "typescript": "~3.5.3"
  }
}
